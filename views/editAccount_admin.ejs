<%include header_admin%>
        <!-- partial -->
        <div class="main-panel">
          <div class="content-wrapper">
            <!-- Page Title Header Ends-->
            <div class="row">   
                <div class="col-12">
                    <div class="jumbotron">
                        <h1 class="display-3" style="font-family: Arial">Thêm tài khoản admin</h1>
                        <hr class="my-2">
                        <% dulieu.forEach(function (item){ %>
                            <form action="/editAccount_admin/<%=item._id %>" method="post">
                            <table class="table table-striped table-inverse table-responsive">
                                    <tbody>
                                        <tr>
                                            <td>Email</td>
                                            <td><input type="email" name="email" value="<%= item.email %>"></td>
                                        </tr>
                                        <tr>
                                            <td>Họ và tên</td>
                                            <td><input type="text" name="fullname" value="<%= item.fullname %>"></td>
                                        </tr>
                                        <tr>
                                            <td>Password</td>
                                            <td><input type="password" name="password" value="<%= item.password %>"></td>
                                        </tr>
                                        <tr>
                                            <td>Confirm password</td>
                                            <td><input type="password" name="confirmpassword"></td>
                                        </tr>
                                        <tr>
                                            <td colspan="2"><button type="submit" class="btn btn-success">Sửa</button></td>
                                        </tr>
                                    </tbody>
                            </table>
                        </form>
                        <% }) %>
                        
                    </div>
                </div>
            </div>
          </div>
          <!-- content-wrapper ends -->
          <!-- partial:partials/_footer.html -->
<% include footer_admin%>