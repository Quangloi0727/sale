<%include header_admin%>
        <!-- partial -->
        <div class="main-panel">
          <div class="content-wrapper">
            <!-- Page Title Header Ends-->
            <div class="row">
               <div class="col-12">
                 <table class="table table-striped table-inverse table-responsive">
                   <thead class="thead-inverse">
                     <tr>
                       <th>Tên sản phẩm</th>
                       <th>Giá</th>
                       <th>Mô tả</th>
                       <th>Nhóm sản phẩm</th>
                       <th>Số lượng còn</th>
                       <th>Ảnh</th>
                       <th>Action</th>
                     </tr>
                     </thead>
                     <tbody>
                      <% dulieu.forEach(function(item){ %>
                        <tr>
                          <td><%= item.tensp %></td>
                          <td><%= item.gia %></td>
                          <td><textarea cols="40" rows="10" disabled><%= item.mota %></textarea></td>
                          <td><%= item.nhomsp %></td>
                          <td><%= item.soluong %></td>
                          <td><img src="/anhsp/<%= item.anhsp %>" alt=""></td>
                          <td><a href="/editProduct/<%= item._id %>"><i class="fa fa-edit" style="font-size: 20px"></i></a> /
                            <a href="/removeProduct/<%= item._id%>" onclick="return xacnhan()"><i style="font-size: 20px" class="fa fa-trash"></i></a>
                         </td> 
                        </tr>
                        <% }) %>
                     </tbody>
                 </table>
               </div>
            </div>
          </div>
          <!-- content-wrapper ends -->
          <!-- partial:partials/_footer.html -->
  <% include footer_admin%>